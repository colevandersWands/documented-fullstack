{
  "name": "refactored-dynamic-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "tree": "depcruise --prefix ../ --include-only \"^\" --collapse '^(node_modules/[^/]+)' --exclude '(sandbox.js|(\\S)+.spec.js|dev-scripts)' --do-not-follow '^node_modules'  --output-type dot ./ | graphviz -Tsvg -odocs/dependency-graph.svg",
    "tree:html": "depcruise --prefix ../ --include-only \"^=\" --exclude '(sandbox.js|(\\S)+.spec.js)' node_modules *.spec.js --output-type dot ./ | graphviz -Tsvg | depcruise-wrap-stream-in-html > docs/dependency-graph.html",
    "document:graphs": "node ./dev-scripts/document.js ../ && npm run tree && npm run tree:html",
    "document:client": "node ./dev-scripts/document client",
    "document:api": "node ./dev-scripts/document api",
    "document": "node ./dev-scripts/document client api"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "tv4": "^1.3.0"
  },
  "devDependencies": {
    "dependency-cruiser": "^10.0.6",
    "graphviz-cli": "^1.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "nodemon": "^2.0.3"
  }
}
